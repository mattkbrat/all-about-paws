{
  "private": true,
  "name": "all-about-paws",
  "description": "Final project for capstone CIS",
  "license": "",
  "scripts": {
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve build",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "1.1.1",
    "@supabase/supabase-js": "^1.28.2",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.1.1",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
